	        char letra = ' ';
	        
	        for(int i = 0; i < romano1.length(); i++){
	                letra = romano1.charAt(i);
	                
	                for(int j = 0; j < Rom.length; j++){
	                    if(letra == Rom[j]){             
	                                suma1 = suma1 + valor [j]; 
	                                
	                            if( ant1 < valor[j]){ 
	                                    suma1 = suma1 - ant1*2 ; 
	                                    ant1 = valor[j]; 
	                                    
	                            }else {
	                        ant1 = valor[j];    
	                            	}
	                    }              

	                }

	        }
	        					
	        				romano2= r.next();
