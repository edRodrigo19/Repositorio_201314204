	        romano1= r.next();
	      
	        char Rom[]= {' ','I','V','X','L','C'}; 
	        int valor[] = {0,1,5,10,50,100};
	        int ant=0, suma1=0, suma2=0, suma3=0, suma4=0, suma5=0;
