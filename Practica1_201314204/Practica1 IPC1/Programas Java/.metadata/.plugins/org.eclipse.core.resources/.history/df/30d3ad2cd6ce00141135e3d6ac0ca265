package Problemas;
import java.util.Scanner;
public class Pro13 {
	
public static void main(String[]args){
	
	int respuesta=0, prueba=0, comparar=0;
	Scanner sc=new Scanner(System.in);

	System.out.println(comparar);
	System.out.println("ingrese cualquier letra del abecedario");
		while (respuesta==0){
			String letra1, letra2;
			
			letra1=sc.next();
			System.out.println("Intente adivinar la letra");
			letra2=sc.next();
			comparar=letra1.compareTo(letra2);
			
		if (comparar<0 ){
			prueba=comparar;
			System.out.println("Antes");
			System.out.println(comparar);	
		}
		else if (prueba>0){
	 		System.out.println("Despues");
	 		System.out.println(prueba);
	 		
			}	
		 		if (comparar==0){
		 			System.out.println("Haz acertado a la letra");
		 			System.out.println(comparar);
		 			respuesta=2;
		 			
		 		}
		}
	
}
}	

